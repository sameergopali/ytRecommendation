{
  "manifest_version": 2,

  "name": "ytRecommendation",
  "description": "kk",
  "version": "1.0",
   "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["start.js"]
    }
  ],
  "permissions": [
          "webNavigation","tabs","storage"
        ],
    "web_accessible_resources": [ "main.js"
			]
 
}